generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String       @id @default(cuid())
  clerkId     String       @unique
  usageCount  Int          @default(0)
  usageLimit  Int          @default(10)
  name        String
  email       String
  imageUrl    String
}
